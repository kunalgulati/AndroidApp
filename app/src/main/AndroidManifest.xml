<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myles.sprint2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/display"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/display"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AdvisorLogin" />
        <activity android:name=".AdvisorEvent" />
        <activity android:name=".AdvisorAdd" />
        <activity android:name=".StudentLogin" />
        <activity android:name=".StudentDepartment" />
        <activity android:name=".StudentFaculty"></activity>
    </application>

</manifest>